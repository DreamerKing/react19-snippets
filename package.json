{
  "name": "react19-snippets",
  "displayName": "React 19 Snippets",
  "description": "Dead simple React snippets you will actually use",
  "version": "0.0.1",
  "publisher": "DreamerKing",
  "icon": "images/react-logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/DreamerKing/react19-snippets.git"
  },
  "keywords": [
    "React",
    "React 19",
    "React 18",
    "React 17",
    "React JS",
    "React 19 Snippets",
    "React Snippets"
  ],
  "engines": {
    "vscode": "^1.63.0"
  },
  "categories": [
    "Snippets"
  ],
  "activationEvents": [
    "onCommand:extension.stayClassy"
  ],
  "main": "./out/src/extension",
  "browser": "./out/src/extension",
  "contributes": {
    "snippets": [
      {
        "language": "javascriptreact",
        "path": "./snippets/snippets.json"
      },
      {
        "language": "javascript",
        "path": "./snippets/snippets.json"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/snippets-ts.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/snippets-ts.json"
      }
    ],
    "commands": [
      {
        "command": "extension.stayClassy",
        "title": "React: class to className"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "test": "node ./node_modules/@vscode/test-electron/out/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.0.12",
    "@types/vscode": "^1.101.0",
    "@vscode/test-electron": "^2.5.2",
    "mocha": "^11.7.1",
    "typescript": "^5.8.3"
  }
}
